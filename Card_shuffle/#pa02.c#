int main(int argc, char ** argv)
{
	
  char deck[MAX_SIZE] = {'A', '2', '3', '4', 
			 '5', '6', '7', '8'};

  CardDeck origdeck;
  bcopy(deck, origdeck.cards, MAX_SIZE * sizeof(char));
  origdeck.size = 5;
  repeatShuffle(origdeck, 3);

	gettimeofday(&end, NULL);
   	fprintf(stderr, "elapsed time: %f\n", ((end.tv_sec*1000000+end.tv_usec) - (start.tv_sec*1000000 + start.tv_usec))/1000000.0);
